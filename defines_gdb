//Number of registers
#define NUM_REGISTERS 72

//Number stack pointer register
#define NUM_REGISTER_SP 14

//Number frame pointer register
#define NUM_REGISTER_FP 30

//Number program counter register
#define NUM_REGISTER_PC 68

//Number first return procedures register
#define NUM_REGISTER_RET1 15

//Number second return procedures register - only SPARC
#define NUM_REGISTER_RET2 31

//Offset add in pc register - only SPARC
#define OFFSET 4